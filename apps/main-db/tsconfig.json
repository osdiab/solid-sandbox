{
	"extends": "@tsconfig/strictest/tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"tsBuildInfoFile": ".tsbuildinfo",
		"noEmit": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"allowJs": true
	},
	"include": ["src/**/*.ts"]
}
