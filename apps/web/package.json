{
  "name": "@solid-sandbox/web",
  "type": "module",
  "scripts": {
    "run:vinxi": "bun run --bun vinxi",
    "dev": "bun run:vinxi dev",
    "build": "bun run:vinxi build",
    "start": "bun run:vinxi start",
    "version": "bun run:vinxi version",
    "prepare": "bun run --bun panda codegen",
    "doppler:run:base": "doppler run --project web --config dev",
    "doppler:run": "bun  doppler:run:base --fallback-only",
    "doppler:refresh": "bun doppler:run:base -- -- echo 'Refreshed doppler secrets'"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.0.10",
    "solid-js": "^1.9.2",
    "vinxi": "^0.4.3"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.47.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/bun": "latest",
    "typescript": "5.6.3",
    "valibot": "^0.42.1",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=18"
  }
}
